{
  "name": "exitpuzzles-museum-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "serve": "serve dist",
    "tail": "tail -f /var/log/syslog | grep exitpuzzles-dash",
    "stop": "sudo systemctl stop exitpuzzles.dash.service",
    "start": "sudo systemctl start exitpuzzles.dash.service",
    "restart": "sudo systemctl restart exitpuzzles.dash.service",
    "status": "sudo systemctl status exitpuzzles.dash.service"
  },
  "dependencies": {
    "core-js": "^3.1.2",
    "firebase": "^7.2.1",
    "vue": "^2.6.10",
    "vue-router": "^3.0.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "vue-template-compiler": "^2.6.10"
  }
}
